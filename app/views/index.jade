	script(type='text/javascript', src='//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.min.js')
	script(type='text/javascript', src='/js/vendors/bootstrap.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.bxslider.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.centralized.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.fixedonlater.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.hashloader.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.mixitup.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.nav.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.parallax-1.1.3.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.responsivevideos.min.js')
	script(type='text/javascript', src='/js/vendors/jquery.scrollTo.min.js')
	script(type='text/javascript', src='/js/application.js')
